        </Grid.Background>
        <Label Content="MIA Input Live...." HorizontalAlignment="Left" Height="26" Margin="32,27,0,0" VerticalAlignment="Top" Width="103" Foreground="#FFE07340"/>
        <Label Content="MIA Output Live...." HorizontalAlignment="Left" Height="26" Margin="366,27,0,0" VerticalAlignment="Top" Width="111" Foreground="#FFE07340"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="751,297,-334,0" VerticalAlignment="Top" Width="100"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="137" Margin="10,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="167"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="137" Margin="329,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="178"/>
        <Label Name="label2" Content="State: Awake" HorizontalAlignment="Left" Height="29" Margin="212,280,0,0" VerticalAlignment="Top" Width="116" Foreground="#FFE07340" FontSize="16" FontFamily="Tahoma"/>
        <ProgressBar x:Name="progress" HorizontalAlignment="Left" Height="14" Margin="20,249,0,0" VerticalAlignment="Top" Width="478"/>

    </Grid>
</Window>
